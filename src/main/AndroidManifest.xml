<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.piskvorky">


    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mynewicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mynewicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".three"> </activity>
        <activity android:name=".four"> </activity>
        <activity android:name=".settings"> </activity>
        <activity android:name=".popup"> </activity>
        <activity android:name=".Score"> </activity>
        <activity android:name=".afterwin" android:theme="@style/AppTheme.custom"></activity>
        <activity android:name=".DisplayItemActivity"> </activity>

        <service android:name=".BackgroundMusic" />
    </application>

</manifest>